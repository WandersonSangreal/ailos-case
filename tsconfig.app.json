/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@environments/*": [
				"environments/*"
			],
			"@modules/*": [
				"app/modules/*"
			],
			"@models/*": [
				"app/models/*"
			],
			"@services/*": [
				"app/services/*"
			]
		},
		"allowSyntheticDefaultImports": true,
		"outDir": "./out-tsc/app",
		"types": []
	},
	"files": [
		"src/main.ts",
		"src/polyfills.ts"
	],
	"include": [
		"src/**/*.d.ts"
	]
}
